package ejercicio2;

public class Autor {

	private int isbn;
	private String titulo;
	private String autor;
	private int nPaginas;
	private String genero;
	
	public Libro( String nombre, String apellido) {
		super();
		this.isbn = isbn;
		this.titulo = titulo;
		this.autor = autor;
		this.nPaginas = nPaginas;
		this.genero = genero;
	}

	
	
	
	
}
