package actividad2;

import javax.swing.JDialog;
import javax.swing.JLabel;
import java.awt.BorderLayout;

public class MensajeOK extends JDialog {
	public MensajeOK() {
		
		
		JLabel mensajePrincipal = new JLabel("New label");
		getContentPane().add(mensajePrincipal, BorderLayout.NORTH);
		
		setSize(400,200);
		setVisible(true);
		setModal(true);
	}

}
